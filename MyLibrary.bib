@article{cassesePartisanDehumanizationAmerican2021,
  title = {Partisan {{Dehumanization}} in {{American Politics}}},
  author = {Cassese, Erin C.},
  year = {2021},
  month = mar,
  journal = {Political Behavior},
  volume = {43},
